<tool id="interactomix_badock" name="BADock" version="0.1">
	<description>
	</description>
	<command interpreter="sh">
		/usr/bin/python /interactomix/bin-tools/BADock.py -a $struct_a -b $struct_b -o $out -x $out_xml 
	</command>
	<environment_variables>
		<environment_variable name="TERM">linux</environment_variable>
	</environment_variables>
	<inputs>
		<param name="struct_a" type="data" format="pdb" label="Structure A"/>
		<param name="struct_b" type="data" format="pdb" label="Structure B"/>
	</inputs>
	<outputs>
		<data name="out"/>
		<data name="out_xml" format="xml"/>
	</outputs>
	<help><![CDATA[
	]]></help>
</tool>
